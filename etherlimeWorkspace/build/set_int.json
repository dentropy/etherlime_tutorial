  "source": "pragma solidity ^0.4.24;\n\ncontract set_int {\n    int256 public public_int;\n    address owner;\n\n    constructor() public {\n        owner = msg.sender;\n        public_int = 42;\n    }\n\n    function set_int_data(int256 tmpNum) public {\n        public_int = tmpNum;\n    }\n}",
